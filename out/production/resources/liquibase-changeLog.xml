<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">


    <changeSet id="create.table.users" author="ruslans.tufetulovs">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="userbank"/>
            </not>
        </preConditions>
        <createTable tableName="userbank">
            <column name="iduser" type="INT" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="firstName" type="VARCHAR(45)"/>
            <column name="lastName" type="VARCHAR(45)"/>
            <column name="age" type="INT"/>
            <column name="typeOfBenefits" type="VARCHAR(45)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>